<form class="container" method="POST">
  <div class="card input-wrapper">

    <div class="from">
      <p>From: 
        <select id="transFrom" name="trans_From">
          <option value="en">English</option>
        </select>
      </p>
    </div>

    <div class="text-area">
      <textarea
        id="input-text"
        cols="30"
        rows="10"
        placeholder="Enter your text here"
        name="inputText"
      ></textarea>
      <button type="submit" class="enter-button">Translate Text</button>
    </div>
  </div>

  <div class="center">
    <div class="swap-position">
      <ion-icon name="swap-horizontal-outline"></ion-icon>
    </div>
  </div>

  <div class="card output-wrapper">
    <div class="to">
      <p>To: 
        <select id="transTo" name="trans_To">
            <option value="es">Spanish</option>
        </select>
      </p>
    </div>

    <textarea
      id="output-text"
      cols="30"
      rows="10"
      placeholder="Translated text will appear here"
      name="outputText"
    >{{translated}}</textarea>
  </div>
</form>

